<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>servlet.auth.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Registration</servlet-name>
        <servlet-class>servlet.auth.Registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Dashboard</servlet-name>
        <servlet-class>servlet.dash.Dashboard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListMembershipApplications</servlet-name>
        <servlet-class>servlet.membership.ListMembershipApplications</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MakePayment</servlet-name>
        <servlet-class>servlet.payments.MakePayment</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListAllMembers</servlet-name>
        <servlet-class>servlet.membership.ListAllMembers</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SubmitClaim</servlet-name>
        <servlet-class>servlet.claims.SubmitClaim</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListClaims</servlet-name>
        <servlet-class>servlet.claims.ListClaims</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListPayments</servlet-name>
        <servlet-class>servlet.payments.ListPayments</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Dashboard</servlet-name>
        <url-pattern>/Dashboard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListMembershipApplications</servlet-name>
        <url-pattern>/ListMembershipApplications</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MakePayment</servlet-name>
        <url-pattern>/MakePayment</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListAllMembers</servlet-name>
        <url-pattern>/ListAllMembers</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SubmitClaim</servlet-name>
        <url-pattern>/SubmitClaim</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListClaims</servlet-name>
        <url-pattern>/ListClaims</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListPayments</servlet-name>
        <url-pattern>/ListPayments</url-pattern>
    </servlet-mapping>
    <session-config>
        <!--This is the timeout for each given session in minutes-->
        <session-timeout>
            20
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>home.jsp</welcome-file>
    </welcome-file-list>
</web-app>
